{
  "name": "edgmont-ui",
  "version": "0.0.51",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://ui.edgmont.dev/",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/:_authToken=${NPM_TOKEN}"
  },
  "bugs": {
    "url": "https://github.com/jbatley/edgmont-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbatley/edgmont-ui"
  },
  "contributors": [
    "joshbatley"
  ],
  "files": [
    "dist/**/*",
    "dist/**/*.css",
    "icons/*",
    "preset.js",
    "package.json"
  ],
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 --no-manager-cache --static-dir .",
    "clean-dist": "rm -rf dist && mkdir dist",
    "clean-icons": "rm -rf icons && mkdir icons",
    "clean": "yarn clean-dist && yarn clean-icons",
    "rollup": "rollup -c rollup.config.js",
    "build": "yarn clean && yarn rollup && yarn build-tailwind",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint": "eslint . --ignore-path .eslintignore",
    "typecheck": "tsc -p tsconfig.build.json",
    "clean-docs": "rm -rf docs && mkdir docs",
    "create-cname": "echo ui.edgmont.dev > ./docs/CNAME",
    "create-ignore": "echo '*\n!.gitkeep' > ./docs/.gitignore",
    "create-keep": "touch ./docs/.gitkeep",
    "create-doc-files": "yarn create-ignore && yarn create-cname && yarn create-keep",
    "copy-preset": "cp preset.js docs/preset.js",
    "build-storybook": "build-storybook -o ./docs",
    "build-docs": "yarn clean-docs && yarn build-storybook && yarn create-doc-files",
    "build-tailwind": "tailwind  -i ./src/index.css -o ./dist/edgmont.css --postcss ./postcss.config.js --minify",
    "build-docs:ci": "yarn clean-docs && yarn build-storybook && yarn create-cname && yarn copy-preset"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@heroicons/react": "^1.0.5",
    "@popperjs/core": "^2.11.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-datepicker": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "jest": "^27.4.1",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4",
    "react-focus-lock": "^2.8.1",
    "react-popper": "^2.2.5",
    "react-simple-code-editor": "^0.11.0",
    "react-spring": "^9.4.3",
    "react-use": "^17.3.2",
    "rollup": "^2.60.1",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "tailwindcss": "^3.0.22",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.22"
  }
}
