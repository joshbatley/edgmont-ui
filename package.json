{
  "name": "edgmont-ui-2",
  "version": "0.0.8",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "dist/**/*.css",
    "package.json"
  ],
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build": "rollup -c rollup.config.js",
    "test": "jest",
    "clean-docs": "rm -rf docs && mkdir docs",
    "create-cname": "echo ui.edgmont.dev > ./docs/CNAME",
    "create-ignore": "echo '*\n!.gitkeep' > ./docs/.gitignore",
    "create-keep": "touch ./docs/.gitkeep",
    "create-doc-files": "yarn create-ignore && yarn create-cname && yarn create-keep",
    "build-storybook": "build-storybook -o ./docs",
    "build-docs": "yarn clean-docs && yarn build-storybook && yarn create-doc-files",
    "build-docs:ci": "yarn clean-docs && yarn build-storybook && yarn create-cname"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.2",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.4.3",
    "@storybook/addon-essentials": "^6.4.3",
    "@storybook/addon-links": "^6.4.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.3",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.1",
    "jest": "^27.4.1",
    "postcss": "^8",
    "postcss-loader": "^6.2.1",
    "rollup": "^2.60.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
