{
  "name": "edgmont-ui",
  "version": "0.0.45",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "dist/**/*.css",
    "package.json"
  ],
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "clean-dist": "rm -rf dist && mkdir dist",
    "rollup": "rollup -c rollup.config.js",
    "build": "yarn clean-dist && yarn rollup",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint": "eslint . --ignore-path .eslintignore",
    "typecheck": "tsc -p tsconfig.build.json",
    "clean-docs": "rm -rf docs && mkdir docs",
    "create-cname": "echo ui.edgmont.dev > ./docs/CNAME",
    "create-ignore": "echo '*\n!.gitkeep' > ./docs/.gitignore",
    "create-keep": "touch ./docs/.gitkeep",
    "create-doc-files": "yarn create-ignore && yarn create-cname && yarn create-keep",
    "build-storybook": "build-storybook -o ./docs",
    "build-docs": "yarn clean-docs && yarn build-storybook && yarn create-doc-files",
    "build-docs:ci": "yarn clean-docs && yarn build-storybook && yarn create-cname"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@popperjs/core": "^2.11.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.5.1",
    "react-focus-lock": "^2.7.1",
    "react-popper": "^2.2.5",
    "react-simple-code-editor": "^0.11.0",
    "react-spring": "^9.3.3",
    "react-use": "^17.3.2",
    "rollup-plugin-import-css": "^3.0.2",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.19",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-datepicker": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "jest": "^27.4.1",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "rollup": "^2.60.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "tailwindcss": "^3.0.0"
  }
}
