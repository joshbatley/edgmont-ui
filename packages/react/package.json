{
  "name": "@edgmont-ui/react",
  "version": "0.1.11",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://ui.edgmont.dev/",
  "bugs": {
    "url": "https://github.com/joshbatley/edgmont-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshbatley/edgmont-ui"
  },
  "contributors": [
    "joshbatley"
  ],
  "files": [
    "dist/**/*",
    "dist/**/*.css",
    "preset.js",
    "package.json"
  ],
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "clean-dist": "rm -rf dist && mkdir dist",
    "vite": "vite build",
    "build": "yarn clean-dist && yarn vite",
    "build:ci": "yarn build",
    "test": "jest --watch",
    "test:ci": "jest",
    "typecheck": "tsc -p tsconfig.build.json",
    "clean-docs": "rm -rf docs && mkdir docs",
    "create-ignore": "echo '*\n!.gitkeep' > ./docs/.gitignore",
    "create-keep": "touch ./docs/.gitkeep",
    "create-doc-files": "yarn create-ignore && yarn create-keep",
    "build-storybook": "build-storybook -o ./docs",
    "build-docs": "yarn clean-docs && yarn build-storybook && yarn create-doc-files",
    "build-docs:ci": "yarn clean-docs && yarn build-storybook",
    "lint": "eslint . --ignore-path .eslintignore"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@heroicons/react": "^2.0.13",
    "@popperjs/core": "^2.11.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.15",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.15",
    "@types/react-datepicker": "^4.3.4",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.0",
    "babel-plugin-macros": "^3.1.0",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-styled-components": "^7.1.1",
    "polished": "^4.2.2",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-focus-lock": "^2.8.1",
    "react-popper": "^2.2.5",
    "react-simple-code-editor": "^0.13.1",
    "react-spring": "^9.4.3",
    "react-use": "^17.3.2",
    "storybook-dark-mode": "^2.0.4",
    "styled-components": "^5.3.6",
    "styled-system": "^5.1.5",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.3",
    "vite-plugin-dts": "^1.7.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "publishConfig": {
    "storybook-dark-mode": "^2.0.4"
  }
}
